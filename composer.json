{
    "name": "ashique-ar/laravel-crud-generator",
    "description": "A powerful, configuration-driven CRUD API generator for Laravel with advanced features including permissions, filtering, sorting, bulk operations, and custom logic handlers.",
    "type": "library",
    "keywords": [
        "laravel",
        "crud",
        "api",
        "generator",
        "rest",
        "permissions",
        "filtering",
        "validation",
        "eloquent",
        "bulk-operations",
        "soft-deletes"
    ],
    "homepage": "https://github.com/ashique-ar/laravel-crud-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashique AR",
            "email": "asqarrsl@gmail.com",
            "homepage": "https://github.com/ashique-ar",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AshiqueAr\\LaravelCrudGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AshiqueAr\\LaravelCrudGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AshiqueAr\\LaravelCrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "CrudGenerator": "AshiqueAr\\LaravelCrudGenerator\\Facades\\CrudGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
